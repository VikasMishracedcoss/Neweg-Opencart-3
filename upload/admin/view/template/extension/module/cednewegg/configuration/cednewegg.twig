                        if (data.success == false && data.message) {
                            $('#auth_error').html(data.message);
                        } else if(data.success == true) {
                            valid = true;
                            $('#auth_error').html('');
                            window.location = redirect_uri;
                        }
                    } catch (e) {
                        valid = false;
                        $('#auth_error').html(e.message);
                    }
                }
            });
        }
        function changePriceVariationType() {
            var val = $("#price_variant_type").val();

            if(val == 'default') {
                $("#price_variation_value_section").hide();
            } else {
                $("#price_variation_value_section").show();
            }
        }

        function generateCronSecureKey(){
          url = 'index.php?route=extension/module/cednewegg/generateCronSecureKey&user_token={{ user_token }}';
          $.ajax({
              url: url,
              dataType: 'json',
              cache: false,
              beforeSend: function() {
                  $('#input-cron_secret').attr('value', '');
              },
              complete: function() {
                  
              },
              success: function(json) {
                  if(json.success){
                    $('#input-cron_secret').attr('value', json.token);
                    $('.cron_secret').html(json.token);
                  }
              },
              error: function(xhr, ajaxOptions, thrownError) {
                  alert(thrownError + "\r\n" + xhr.statusText + "\r\n" + xhr.responseText);
              }
          });
        }

    </script>
