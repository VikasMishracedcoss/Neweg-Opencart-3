                $('#dev_detail').show();
            }
        }
        function authorizeAccount() {
            $('#auth_error').html('');
            var url = 'index.php?route=extension/module/cednewegg/authorize&user_token={{ user_token }}';
            var valid = false;
            $.ajax({
                type: 'POST',
                url: url,
                success: function (data) {
                    try {
                        if (data.success == false && data.message) {
                            $('#auth_error').html(data.message);
                        } else if(data.success == true) {
                            valid = true;
                            $('#auth_error').html();
                            var url = data.message;
                            window.open(url, '_blank');
                        }
                    } catch (e) {
                        valid = false;
                        $('#auth_error').html(e.message);
                    }
                }
            });
        }
